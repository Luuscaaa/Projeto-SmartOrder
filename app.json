{
  "expo": {
    "name": "App-Smart-Order",
    "slug": "App-Smart-Order-V2",
    "owner": "luscaa",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-inicial.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-inicial.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.luscaa.AppSmartOrder"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo-inicial.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "0d56cffe-caf7-4dfe-b682-65b2d2578089"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
